#creating a table with student details like roll no.,names and marks
rec=[]

rec.append([1,'RAM',99])
rec.append([2,'Garg',97])
rec.append([3,'Arjun',93])
rec.append([4,'Rajesh',49])

print("Records present in list:")

for a in range(len(rec)):
    for b in range(len(rec[a])):
        print(rec[a][b],end="\t")
    print()
    print("\nSecond record in the list:")
#Printing the Second Record from the list

for b in range(len(rec[1])):
    print(rec[1][b], end="\t")